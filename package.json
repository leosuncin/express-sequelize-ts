{
  "name": "express-sequelize-ts",
  "version": "1.0.0",
  "private": true,
  "description": "API REST with Express, Sequelize & TypeScript",
  "keywords": [],
  "license": "GPL-3.0",
  "author": "Jaime Leonardo Suncin Cruz <leosuncin@gmail.com>",
  "scripts": {
    "build": "tsc; tsc-alias",
    "build:openapi": "openapi-comment-parser src dist/openapi.json",
    "check:health": "node dist/health-check.js",
    "dev": "tsx watch --clear-screen=false -r dotenv/config src/index.ts | pino-colada",
    "format": "prettier --cache --ignore-path .gitignore -wu .",
    "lint": "eslint --cache --ignore-path .gitignore --ext .ts,.js .",
    "prepare": "husky install",
    "start": "node dist/index.js",
    "test": "jest"
  },
  "dependencies": {
    "@godaddy/terminus": "4.12.1",
    "envalid": "8.0.0",
    "express": "4.18.2",
    "express-async-handler": "1.2.0",
    "express-paginate": "1.0.2",
    "http-errors": "2.0.0",
    "http-status-codes": "2.3.0",
    "mysql2": "3.6.3",
    "openapi-comment-parser": "1.0.0",
    "pino-http": "8.5.1",
    "sequelize": "6.34.0",
    "swagger-ui-express": "5.0.0",
    "zod": "3.22.4",
    "zod-validation-error": "2.1.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.2",
    "@types/express": "4.17.21",
    "@types/express-paginate": "1.0.4",
    "@types/http-errors": "2.0.4",
    "@types/jest": "^29.5.7",
    "@types/node": "20.8.10",
    "@types/pino-http": "5.8.3",
    "@types/supertest": "^2.0.15",
    "@types/swagger-ui-express": "4.1.5",
    "dotenv": "16.3.1",
    "eslint": "8.53.0",
    "eslint-config-galex": "4.5.2",
    "husky": "8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "15.0.2",
    "markdownlint-cli2": "0.10.0",
    "pino-colada": "2.2.2",
    "prettier": "3.0.3",
    "sequelize-cli": "6.6.2",
    "sort-package-json": "2.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "1.8.8",
    "tsx": "3.14.0",
    "typescript": "5.0.4"
  }
